DROP TABLE IF EXISTS `test_course`;
CREATE TABLE `test_course` (
  `Id` int(11) NOT NULL AUTO_INCREMENT,
  `name` varchar(50) NOT NULL DEFAULT '',
  `class_room` varchar(30) NOT NULL DEFAULT '',
  `teacher` varchar(30) NOT NULL DEFAULT '',
  `course_start_time` datetime NOT NULL,
  `course_end_time` datetime NOT NULL,
  `class_start_time` datetime NOT NULL,
  `class_end_time` datetime NOT NULL,
  `score` int(11) NOT NULL DEFAULT 10,
  `class_rl` int(11) NOT NULL,
  PRIMARY KEY (`Id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

